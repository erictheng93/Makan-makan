# API 配置
VITE_API_BASE_URL=https://api.makanmakan.app
VITE_WS_BASE_URL=wss://api.makanmakan.app
VITE_IMAGE_BASE_URL=https://images.makanmakan.app

# 應用配置
VITE_APP_NAME=MakanMakan
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=production

# 功能開關
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# 第三方服務
VITE_SENTRY_DSN=
VITE_GA_TRACKING_ID=
VITE_HOTJAR_ID=

# 開發配置
VITE_MOCK_API=false
VITE_DEBUG_MODE=false

# PWA 配置
VITE_PWA_NAME=MakanMakan
VITE_PWA_SHORT_NAME=MakanMakan
VITE_PWA_DESCRIPTION=便捷的餐廳點餐系統，掃描 QR Code 即可開始點餐

# 緩存配置
VITE_CACHE_DURATION=300000
VITE_IMAGE_CACHE_DURATION=604800000